    ViewBag.Title = "Ana Sayfa";
    Layout = "~/Views/Shared/_LayoutUser.cshtml";

    var brandService = DependencyResolver<IBrandService>.Resolve();
    var numberOfBrand = brandService.GetAllApplications(CurrentUser.Identity.UserId.ToString(), true).Count;

    var patentService = DependencyResolver<IPatentService>.Resolve();
    var numberOfPatent = patentService.GetAllApplications(CurrentUser.Identity.UserId.ToString()).Count;

    var industrialDesignService = DependencyResolver<IIndustrialDesignService>.Resolve();
    var numberOfDesign = industrialDesignService.GetAllApplications(CurrentUser.Identity.UserId.ToString()).Count;

    var numberOfBrandWatch = brandService.GetAllBrandWatchApplications(CurrentUser.Identity.UserId.ToString()).Count;

    var numberOfOtherBrand = brandService.GetAllApplications(
        CurrentUser.Identity.UserId.ToString(), false).Count;

    var domainService = DependencyResolver<IDomainService>.Resolve();
    var numberOfDomain = domainService.GetAllDomainApplications(CurrentUser.Identity.UserId.ToString()).Count;
}
